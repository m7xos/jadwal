APP_NAME="Jadwal"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=https://your-domain.test
SHOW_ADMIN_PANEL_MENU=true

APP_LOCALE=id
APP_FALLBACK_LOCALE=id
APP_FAKER_LOCALE=id_ID

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database
# PHP_CLI_SERVER_WORKERS=4
BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=jadwal_db
DB_USERNAME=jadwal_user
DB_PASSWORD=your_strong_password

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database
CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# wa-gateway (WhatsApp Gateway) - Wablas-compatible API
# Default: http://localhost:5001 (lihat /home/wa-gateway/docker-compose.yaml)
WA_GATEWAY_BASE_URL=http://localhost:5001
# KEY pada wa-gateway (jika di-set). Jika kosong, header `key` tidak akan dikirim.
WA_GATEWAY_KEY=
# Token device dari panel wa-gateway (dipakai sebagai "token" ala Wablas)
WA_GATEWAY_TOKEN=
# Secret key tidak dipakai oleh wa-gateway, tapi tetap disediakan untuk kompatibilitas.
WA_GATEWAY_SECRET_KEY=
# Webhook receiver di aplikasi ini:
# Set di wa-gateway sebagai webhookBaseUrl = ${APP_URL}/wa-gateway/webhook
# (wa-gateway akan POST ke ${webhookBaseUrl}/message)
# Default grup dikonfigurasi lewat data Grup WA (tandai sebagai default)
WA_GATEWAY_GROUP_1_ID=
WA_GATEWAY_GROUP_2_ID=
WA_GATEWAY_GROUP_3_ID=

WA_GATEWAY_FINISH_WHITELIST=

# PDF to text (Ubuntu)
PDFTOTEXT_PATH=/usr/bin/pdftotext

# Default admin seed (optional; ubah di prod)
DEFAULT_ADMIN_EMAIL=admin@example.com
DEFAULT_ADMIN_PASSWORD=changeme123
DEFAULT_PERSONIL_NIP=0000000000000000
DEFAULT_PERSONIL_PHONE=6281111111111
DEFAULT_PERSONIL_NAME=Administrator

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
